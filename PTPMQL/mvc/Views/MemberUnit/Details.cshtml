@model mvc.Models.Entities.MemberUnit

@{
    ViewData["Title"] = "Details";
}

<h2>Details</h2>
<div>
    <h4>MemberUnit</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Name</dt>
        <dd class="col-sm-10">@Model.Name</dd>
        <dt class="col-sm-2">Address</dt>
        <dd class="col-sm-10">@Model.Address</dd>
        <dt class="col-sm-2">PhoneNumber</dt>
        <dd class="col-sm-10">@Model.PhoneNumber</dd>
        <dt class="col-sm-2">WebsiteUrl</dt>
        <dd class="col-sm-10">@Model.WebsiteUrl</dd>
    </dl>
</div>
<a asp-action="Edit" asp-route-id="@Model.MemberUnitId" class="btn btn-primary">Edit</a>
<a asp-action="Index" class="btn btn-secondary">Back to List</a> 